{
  "name": "deefour/transformer",
  "description": "Transform raw input data into consistent, immutable PHP objects",
  "homepage": "https://github.com/deefour/interactor",
  "keywords": ["transformer", "deefour", "request data", "raw input", "laravel"],
  "license": "MIT",
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "phpspec/phpspec": "^3.1",
    "friendsofphp/php-cs-fixer": "dev-master"
  },
  "authors": [
    {
      "name": "Jason Daly",
      "email": "jason@deefour.me"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Deefour\\Transformer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Deefour\\Transformer\\Stub\\": "stub/"
    }
  }
}
